pair< vector<pair<int, int> >, vector<pair<int, int> > >  calculate_next_points(int n, Drone& drone,  Monster visible_monster)
{
    int     i;
    vector<pair<int, int>> drone_next_points;
    vector<pair<int, int>> monster_next_points;
    pair< vector<pair<int, int>>, vector<pair<int, int> > > coordinates;
    double     t;
 
    i = 0;

    visible_monster.vx += visible_monster.x; 
    visible_monster.vy += visible_monster.y;
    // drone.next_x += drone.x;
    // drone.next_y += drone.y;
    while (i <= n)
    {
        t = (double)i / n;

        drone_next_points.push_back(make_pair(drone.x + t * (drone.next_x - drone.x), drone.y + t * (drone.next_y- drone.y)));

        monster_next_points.push_back(make_pair(visible_monster.x + t * (visible_monster.vx - visible_monster.x), 
                                                visible_monster.y + t * (visible_monster.vy - visible_monster.y)));

        i++;
    }
    // coordinates.first = drone_next_points;
    // coordinates.second = monster_next_points;
    return (make_pair(drone_next_points, monster_next_points));
}

